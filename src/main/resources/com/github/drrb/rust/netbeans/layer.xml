<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>

    <folder name="Editors">
        <folder name="text">
            <folder name="x-rust-source">
                <attr name="displayName" bundlevalue="com.github.drrb.rust.netbeans.Bundle#Editors/text/x-rust-source"/>
